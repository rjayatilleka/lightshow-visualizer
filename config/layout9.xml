<?xml version="1.0"?>
<layout>
    <size width="800" height="600"/>
    <channels count="16"/>
    <background red="75" green="75" blue="75" />
    <elements>
        <group x="300" y="500"><!-- Cylinder is a 200x100 box -->
            <group x="0" y="0"><!-- Cylinder Red is 180x100 -->
  	            <shape type="line" name="Cylinder Red 1" x="0" y="0" x2="40" y2="100" red="255" green="0" blue="0" relative="true" channel="0" />
  	            <shape type="line" name="Cylinder Red 2" x="20" y="0" x2="40" y2="100" red="255" green="0" blue="0" relative="true" channel="1" />
  	            <shape type="line" name="Cylinder Red 3" x="40" y="0" x2="40" y2="100" red="255" green="0" blue="0" relative="true" channel="2" />
  	            <shape type="line" name="Cylinder Red 4" x="60" y="0" x2="40" y2="100" red="255" green="0" blue="0" relative="true" channel="3" />
  	            <shape type="line" name="Cylinder Red 5" x="80" y="0" x2="40" y2="100" red="255" green="0" blue="0" relative="true" channel="4" />
  	            <shape type="line" name="Cylinder Red 6" x="100" y="0" x2="40" y2="100" red="255" green="0" blue="0" relative="true" channel="5" />
  	            <shape type="line" name="Cylinder Red 7" x="120" y="0" x2="40" y2="100" red="255" green="0" blue="0" relative="true" channel="6" />
  	            <shape type="line" name="Cylinder Red 8" x="140" y="0" x2="40" y2="100" red="255" green="0" blue="0" relative="true" channel="7" />
            </group>
            <group x="10" y="0"><!-- Cylinder Green is 180x100, offset 10 from Red -->
  	            <shape type="line" name="Cylinder Green 1" x="0" y="0" x2="40" y2="100" green="255" red="0" blue="0" relative="true" channel="8" />
  	            <shape type="line" name="Cylinder Green 2" x="20" y="0" x2="40" y2="100" green="255" red="0" blue="0" relative="true" channel="9" />
  	            <shape type="line" name="Cylinder Green 3" x="40" y="0" x2="40" y2="100" green="255" red="0" blue="0" relative="true" channel="10" />
  	            <shape type="line" name="Cylinder Green 4" x="60" y="0" x2="40" y2="100" green="255" red="0" blue="0" relative="true" channel="11" />
  	            <shape type="line" name="Cylinder Green 5" x="80" y="0" x2="40" y2="100" green="255" red="0" blue="0" relative="true" channel="12" />
  	            <shape type="line" name="Cylinder Green 6" x="100" y="0" x2="40" y2="100" green="255" red="0" blue="0" relative="true" channel="13" />
  	            <shape type="line" name="Cylinder Green 7" x="120" y="0" x2="40" y2="100" green="255" red="0" blue="0" relative="true" channel="14" />
  	            <shape type="line" name="Cylinder Green 8" x="140" y="0" x2="40" y2="100" green="255" red="0" blue="0" relative="true" channel="15" />
            </group>
            <group x="0" y="0"><!-- Cylinder Blue is 180x100 -->
  	            <shape type="line" name="Cylinder Blue 1" x="0" y="100" x2="40" y2="0" blue="255" red="0" green="0" relative="true" channel="16" />
  	            <shape type="line" name="Cylinder Blue 2" x="20" y="100" x2="40" y2="0" blue="255" red="0" green="0" relative="true" channel="17" />
  	            <shape type="line" name="Cylinder Blue 3" x="40" y="100" x2="40" y2="0" blue="255" red="0" green="0" relative="true" channel="18" />
  	            <shape type="line" name="Cylinder Blue 4" x="60" y="100" x2="40" y2="0" blue="255" red="0" green="0" relative="true" channel="19" />
  	            <shape type="line" name="Cylinder Blue 5" x="80" y="100" x2="40" y2="0" blue="255" red="0" green="0" relative="true" channel="20" />
  	            <shape type="line" name="Cylinder Blue 6" x="100" y="100" x2="40" y2="0" blue="255" red="0" green="0" relative="true" channel="21" />
  	            <shape type="line" name="Cylinder Blue 7" x="120" y="100" x2="40" y2="0" blue="255" red="0" green="0" relative="true" channel="22" />
  	            <shape type="line" name="Cylinder Blue 8" x="140" y="100" x2="40" y2="0" blue="255" red="0" green="0" relative="true" channel="23" />
            </group>
            <group x="10" y="0"><!-- Cylinder White is 180x100, offset 10 from Blue -->
  	            <shape type="line" name="Cylinder White 1" x="0" y="100" x2="40" y2="0" blue="255" red="255" green="255" relative="true" channel="24" />
  	            <shape type="line" name="Cylinder White 2" x="20" y="100" x2="40" y2="0" blue="255" red="255" green="255" relative="true" channel="25" />
  	            <shape type="line" name="Cylinder White 3" x="40" y="100" x2="40" y2="0" blue="255" red="255" green="255" relative="true" channel="26" />
  	            <shape type="line" name="Cylinder White 4" x="60" y="100" x2="40" y2="0" blue="255" red="255" green="255" relative="true" channel="27" />
  	            <shape type="line" name="Cylinder White 5" x="80" y="100" x2="40" y2="0" blue="255" red="255" green="255" relative="true" channel="28" />
  	            <shape type="line" name="Cylinder White 6" x="100" y="100" x2="40" y2="0" blue="255" red="255" green="255" relative="true" channel="29" />
  	            <shape type="line" name="Cylinder White 7" x="120" y="100" x2="40" y2="0" blue="255" red="255" green="255" relative="true" channel="30" />
  	            <shape type="line" name="Cylinder White 8" x="140" y="100" x2="40" y2="0" blue="255" red="255" green="255" relative="true" channel="31" />
            </group>
        </group>

        <!-- Megatree, channels 32 - 43, 200x100 box, right above cylinder -->
        <shape type="megatree" name="Megatree" x="300" y="400" width="200" height="100" channel="32">
            <spoke red="0" green="0" blue="255"/>
            <spoke red="0" green="255" blue="0"/>
            <spoke red="255" green="255" blue="255"/>
            <spoke red="0" green="0" blue="255"/>
            <spoke red="0" green="255" blue="0"/>
            <spoke red="255" green="255" blue="255"/>
            <spoke red="0" green="0" blue="255"/>
            <spoke red="0" green="255" blue="0"/>
            <spoke red="255" green="255" blue="255"/>
            <spoke red="0" green="0" blue="255"/>
            <spoke red="0" green="255" blue="0"/>
            <spoke red="255" green="255" blue="255"/>
        </shape>

        <!-- Big M, channels 44-47 -->
        <group x="120" y="540">
          <shape type="rect" name="M Green Outer Border" x="0" y="0" width="60" height="60" red="0" green="255" blue="0" fill="false" channel="44" />
          <shape type="rect" name="M Blue Inner Border" x="5" y="5" width="50" height="50" red="0" green="0" blue="255" fill="false" channel="45" />

          <!-- Red M -->
          <shape type="text" name="M Red" x="8" y="8"
          red="255" green="0" blue="0" font="Times New Roman"
          fontSize="30" channel="46">  M  </shape>

          <!-- Gold M -->
          <shape type="text" name="M Gold" x="12" y="12"
          red="255" green="215" blue="0" font="Times New Roman"
          fontSize="30" channel="47">  M  </shape>
        </group>

        <!-- Lower left drapes -->
        <group x="0" y="480">
          <shape type="arc" name="Lower Drape Left 1 Blue" x="0" y="0" width="40" height="120" red="0" green="0" blue="255" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Left 1 Yellow" x="3" y="3" width="40" height="120" red="255" green="215" blue="0" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Left 2 Red" x="20" y="20" width="40" height="120" red="255" green="0" blue="0" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Left 2 White" x="23" y="23" width="40" height="120" red="255" green="255" blue="255" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Left 3 Blue" x="40" y="40" width="40" height="120" red="0" green="0" blue="255" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Left 3 Yellow" x="43" y="43" width="40" height="120" red="255" green="215" blue="0" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Left 4 Red" x="60" y="60" width="40" height="120" red="255" green="0" blue="0" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Left 4 White" x="63" y="63" width="40" height="120" red="255" green="255" blue="255" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Left 5 Blue" x="80" y="80" width="40" height="120" red="0" green="0" blue="255" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Left 5 Yellow" x="83" y="83" width="40" height="120" red="255" green="215" blue="0" fill="false" start="180" size="180" channel="48" />
        </group>

        <!-- Lower left drapes -->
        <group x="180" y="480">
          <shape type="arc" name="Lower Drape Right 5 Blue" x="0" y="0" width="40" height="120" red="0" green="0" blue="255" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Right 5 Yellow" x="3" y="3" width="40" height="120" red="255" green="215" blue="0" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Right 4 Red" x="20" y="20" width="40" height="120" red="255" green="0" blue="0" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Right 4 White" x="23" y="23" width="40" height="120" red="255" green="255" blue="255" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Right 3 Blue" x="40" y="40" width="40" height="120" red="0" green="0" blue="255" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Right 3 Yellow" x="43" y="43" width="40" height="120" red="255" green="215" blue="0" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Right 2 Red" x="60" y="60" width="40" height="120" red="255" green="0" blue="0" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Right 2 White" x="63" y="63" width="40" height="120" red="255" green="255" blue="255" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Right 1 Blue" x="80" y="80" width="40" height="120" red="0" green="0" blue="255" fill="false" start="180" size="180" channel="48" />
          <shape type="arc" name="Lower Drape Right 1 Yellow" x="83" y="83" width="40" height="120" red="255" green="215" blue="0" fill="false" start="180" size="180" channel="48" />
        </group>
    </elements>
</layout>
